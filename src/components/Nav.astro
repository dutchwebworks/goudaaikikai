---
import { getCollection } from "astro:content";

const navData = await getCollection("nav");
---

<ul id="nav">
    {
        navData.map(({ data: { url, title, label } }) => (
            <li>
                <a href={url} title={title}>
                    {label}
                </a>
            </li>
        ))
    }
</ul>
